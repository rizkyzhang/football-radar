{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": "off",
        "import/prefer-default-export": "off",
        "indent": ["error", 2, { "ignoredNodes": ["TemplateLiteral *"] }],
        "no-console": "off",
        "no-restricted-globals": "off",
        "quotes": ["error", "double"]
    }
}
